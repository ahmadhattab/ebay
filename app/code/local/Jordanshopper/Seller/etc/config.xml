<?xml version="1.0"?>
<config>
	<modules>
		<Jordanshopper_Seller>
			<version>1.0.1.2</version>
		</Jordanshopper_Seller>
	</modules>
	<frontend>
		<routers>
			<seller>
				<use>standard</use>
				<args>
					<module>Jordanshopper_Seller</module>
					<frontName>seller</frontName>
				</args>
			</seller>
		</routers>
		<layout>
			<updates>
				<seller>
					<file>seller.xml</file>
				</seller>
			</updates>
		</layout>
	</frontend>
	<global>
		<blocks>
			<seller>
				<class>Jordanshopper_Seller_Block</class>
			</seller>
		</blocks>
	</global>
</config>